{
    "name": "matbcvo/laravel-token-auth",
    "type": "library",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Martin Voorem√§e",
            "email": "martin.vooremae@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "phpstan/phpstan": "^2.0",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^9.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Matbcvo\\LaravelTokenAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matbcvo\\LaravelTokenAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matbcvo\\LaravelTokenAuth\\LaravelTokenAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    }
}